<body>
  <section id="title">
    <div class="container-fluid">

      <!-- nav bar -->
      <nav class="navbar navbar-expand-lg">

        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggle-icon"></span>
        </button>
        <a class="navbar-brand" href="">HEALTHY CARE</a>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav ms-auto">
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="Home">HOME</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/about-us">ABOUT US</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/service">SERVICES</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/myaccount-show-appointment">MY ACCOUNT</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/feedback">FEEDBACK</a>
            </li>
            <li class="cart-icon">
              <i class="fa fa-shopping-cart" aria-hidden="true"></i>
            </li>
          </ul>
        </div>
      </nav>

      <!-- location and ad img -->
      <div class="row">
        <div class="col-lg-6 sm-12">
          <h1>Welcome to Healthy Care Massage.</h1>
          <h2 class="business-info"></h2>
          <h3 class="business-info"></h3>

        </div>
        <div class="col-lg-6 sm-12">

          <div id="advertise" class="carousel slide" data-bs-ride="true">
            <div class="carousel-indicators">
              <button type="button" data-bs-target="#advertise" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
              <button type="button" data-bs-target="#advertise" data-bs-slide-to="1" aria-label="Slide 2"></button>
              <button type="button" data-bs-target="#advertise" data-bs-slide-to="2" aria-label="Slide 3"></button>
            </div>
            <div class="carousel-inner">
              <div class="carousel-item active">
                <img class="mainad" src="images/attractive-african-woman-enjoying-face-massage-spa-salon.jpg" alt="logo" height="500" width="650">
              </div>
              <div class="carousel-item">
                <img class="mainad" src="images/deep-tissue-.jpg" alt="logo" height="500" width="650">
              </div>
              <div class="carousel-item">
                <img class="mainad" src="images/Discover-Massage.png" alt="logo" height="500" width="650">
              </div>
            </div>
            <button class="carousel-control-prev" type="button" href="#advertise" data-bs-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="visually-hidden">Previous</span>


            </button>
            <button class="carousel-control-next" type="button" href="#advertise" data-bs-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="visually-hidden">Next</span>

            </button>
          </div>



        </div>

      </div>



    </div>




  </section>
  <section id="location">
    <div class="location-booking">


      <div class="row">
        <div class="col-lg-6 sm-12">
          <div class="card">
            <div class="card-header">
              <h4>Casuarina Shop</h4>
            </div>
            <div class="card-body">
              <h5 class="location-detail">Location Details</h5>
              <p>
              <p>Shop GD118 Casuarina Square, </p>
              <p>247 Trower Rd, Casuarina, NT, 0810</p>



              <h5 class="open-hours">Opening Hours</h5>
              <p class="card-text">
              <p> Monday - Thursday : 9:00 am - 5:30 pm</p>
              <p> Friday : 9:00 am - 9:00 pm</p>
              <p> Saturday : 9:00 am - 5:00 pm</p>
              <p> Sunday : 10:00 am - 3:00 pm</p>


              <a href="/appointment-cas">
                <button class="btn">Book Online</button>
              </a>
            </div>
          </div>
        </div>
        <div class="col-lg-6 sm-12">
          <div class="card">
            <div class="card-header">
              <h4>Gateway Shop</h4>
            </div>
            <div class="card-body">
              <h5 class="location-detail">Location Details</h5>
              <p>
              <p>Shop T54, A1, Gateway Shopping Centre, </p>
              <p>Yarrawonga, NT, 0830</p>



              <h5 class="open-hours">Opening Hours</h5>
              <p class="card-text">
              <p> Monday - Thursday : 9:00 am - 5:00 pm</p>
              <p> Friday : 9:00 am - 7:00 pm</p>
              <p> Saturday : 9:00 am - 5:00 pm</p>
              <p> Sunday : 10:00 am - 4:00 pm</p>


              <a href="#" class="btn">Book Online</a>
            </div>
          </div>
        </div>
      </div>



    </div>
  </section>
  <section class="shop-container">
    <h3 class="shop-title">Products</h3>
    <div class="shop-content">
      <!-- box1 -->
      <div class="product-box">
        <img src="images/gift voucher.jpg" alt="gift voucher" class="product-img">
        <h6 class="product-title">$85 Gift Voucher 1 Hour</h6>
        <span class="product-price">$85</span>
        <button class="Purchase">Add to cart</button>
      </div>



      <!-- box2 -->
      <div class="product-box">
        <img src="images/gift voucher.jpg" alt="gift voucher" class="product-img">
        <h6 class="product-title">$240 Family Voucher 1 Hour</h6>
        <span class="product-price">$240</span>
        <button class="Purchase">Add to cart</button>

      </div>
      <!-- box3 -->
      <div class="product-box">
        <img src="images/gift voucher.jpg" alt="gift voucher" class="product-img">
        <h6 class="product-title">$155 Couple Voucher 1 Hour</h6>
        <span class="product-price">$155</span>
        <button class="Purchase">Add to cart</button>

      </div>
      <div class="product-box">
        <img src="images/ear candling.jpg" alt="ear candle" class="product-img">
        <h6 class="product-title">$25 10pc Ear Candle</h6>
        <span class="product-price">$25</span>
        <button class="Purchase">Add to cart</button>

      </div>
      <div class="product-box">
        <img src="images/essential-oil.png" alt="massage oil" class="product-img">
        <h6 class="product-title">$18 Massage Oil</h6>
        <span class="product-price">$18</span>
        <button class="Purchase">Add to cart</button>

      </div>
      <div class="product-box">
        <img src="images/candle.png" alt="candle" class="product-img">
        <h6 class="product-title">$32 Scented Candle</h6>
        <span class="product-price">$32</span>
        <button class="Purchase">Add to cart</button>

      </div>
    </div>


  </section>
  <section class="cart">
    <div class="cart-head">
      <h3 class="cart-title">Shopping Cart</h3>
    </div>
    <div class="cart-content">

      <div class="cart-box">
        <img src="images/gift voucher.jpg" alt="" class="product-img">

        <div class="details-box">
          <div class="cart-product-title">$85 Gift Voucher 1 Hour</div>
          <div class="cart-price">$85</div>
          <input type="number" value="1" class="cart-quantity">

        </div>



        <i class="fa fa-trash cart-remove" aria-hidden="true"></i>

      </div>
    </div>
    <!-- total -->
    <div class="total">
      <div class="total-title">Total</div>
      <div class="total-price">$0</div>

    </div>
    <!-- buy button -->
    <button type="button" class="btn-buy">Buy Now</button>
    <!-- cart close -->
    <div class=closecart>
      <i class="fa fa-window-close" id="close-cart" aria-hidden="true"></i>
    </div>





  </section>


</body>
